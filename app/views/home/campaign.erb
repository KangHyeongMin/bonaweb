    <!-- About Section -->
    <section id="about" class="about-section">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="jumbotron">
                        <!--<div class="row">-->
                        <h1><img src="/assets/bona_logo.png" style="width:20%; vertical-align:top"> Campaign</h1>
                        <!--</div>-->
                        <p class="lead">오늘 한 운동을 기록해 주세요</p>
                    
                        <form action="/home/write_action" method="POST">
                            <input type="hidden" name="username" value="강형민">
                            <div class="btn-group" data-toggle="buttons">
                              <label class="btn btn-lg btn-danger">
                                <input type="radio" name="kind" value="푸시업" id="option1" autocomplete="off"> 푸시업
